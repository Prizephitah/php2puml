{
	"name": "prizephitah/php2puml",
	"description": "Parses PHP code and produces PlantUML code representing it.",
	"type": "library",
	"require": {
		"nikic/php-parser": "^4.4",
		"twig/twig": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.1"
	},
	"autoload": {
		"psr-4": {
			"Prizephitah\\php2puml\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Prizephitah\\php2puml\\Test\\": "tests/"
		}
	},
	"prefer-stable": true
}